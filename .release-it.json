{
  "git": {
    "requireBranch": "main",
    "addUntrackedFiles": true,
    "commitMessage": "bump: release v1.7.3",
    "requireCleanWorkingDir": false,
    "requireUpstream": true,
    "pushRepo": "git@github.com:reyhappen/react-easy-sort.git",
    "pushArgs": ["--follow-tags"]
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": true,
    "publishPath": "./dist/",
    "publishArgs": ["--access=public"],
    "allowSameVersion": true
  }
}