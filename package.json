{
  "name": "@reyhappen/react-easy-sort",
  "version": "1.7.4",
  "description": "A React component to sort items in lists or grids",
  "homepage": "https://reyhappen.github.io/react-easy-sort/",
  "keywords": [
    "react",
    "sort",
    "sortable",
    "sortable grid",
    "sortable list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reyhappen/react-easy-sort.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Valentin Hervieu",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "start": "yarn storybook",
    "version": "yarn && yarn build",
    "build": "tsup && yarn-or-npm run build:copy-files",
    "test": "yarn-or-npm run unit && yarn-or-npm run lint && yarn-or-npm run lint:ts",
    "unit": "jest src",
    "unit:watch": "jest --watchAll src",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:ts": "tsc --noEmit",
    "prebuild": "rimraf dist",
    "build:copy-files": "node ./scripts/copy-build-files.mjs",
    "prepublishOnly": "yarn-or-npm run build",
    "precommit": "lint-staged",
    "format": "prettier --write src/**/*.ts* stories/**/*.ts*",
    "npm:publish": "np --contents=dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "release:dry": "release-it --dry-run --no-npm",
    "release:info": "release-it --release-version",
    "release:beta:no-npm": "release-it --preRelease=beta --no-npm",
    "release:beta": "release-it --preRelease=beta",
    "release:no-npm": "release-it --no-npm",
    "release": "release-it --ci --no-git.requireUpstream"
  },
  "peerDependencies": {
    "react": ">=16.4.0",
    "react-dom": ">=16.4.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-object-rest-spread": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@mui/styles": "^5.15.15",
    "@storybook/addon-actions": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/addon-themes": "^8.0.8",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.8",
    "@storybook/cli": "^8.0.8",
    "@storybook/manager-api": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-webpack5": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "ajv": "^8.12.0",
    "all-contributors-cli": "^6.26.1",
    "array-move": "^4.0.0",
    "babel-loader": "^9.1.3",
    "caniuse-lite": "^1.0.30001610",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "np": "^10.0.3",
    "prettier": "^3.2.5",
    "query-string": "^9.0.0",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^17.2.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.0.8",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "yarn-or-npm": "^3.0.1"
  },
  "lint-staged": {
    "*.+(ts|tsx|js|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest"
  }
}
